@{
}
@model IEnumerable<University>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Universidades</title>
</head>
<body>
    <h1>Universidades</h1>
    <ul>
        @foreach (var university in Model)
        {
            <li>

                <a href="@Url.Action("Carreras", "Home", new { universityId = university.Id })">
                    @* <img src="@university.LogoUrl" alt="@university.Name" /> *@
                </a>
                <p>@university.Name</p>
                <p>@university.Description</p>
                <p>@university.Id</p>
            </li>
        }
    </ul>
</body>
</html>